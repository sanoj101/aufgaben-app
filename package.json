{
  "name": "aufgaben-app",
  "version": "1.0.0",
  "description": "Aufgabenverwaltung mit Push-Benachrichtigungen",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate-vapid": "node -e \"const webpush = require('web-push'); const vapidKeys = webpush.generateVAPIDKeys(); console.log('Public Key:', vapidKeys.publicKey); console.log('Private Key:', vapidKeys.privateKey);\""
